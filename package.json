{
  "name": "catalyst",
  "version": "1.0.0",
  "license": "MIT",
  "private": "true",
  "workspaces": [
    "workspaces/backend",
    "workspaces/shared-utils",
    "workspaces/ui"
  ],
  "scripts": {
    "build": "yarn workspace ui run build",
    "deploy:backend": "yarn workspace backend run deploy",
    "deploy:ui": "yarn workspace ui run deploy",
    "destroy": "yarn workspace backend run destroy",
    "start": "concurrently -n \"backend,ui\" \"yarn workspace backend start\" \"yarn workspace ui start\""
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "concurrently": "^7.6.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
